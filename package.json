{
  "name": "app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "prebuild": "expo prebuild",
    "android": "DARK_MODE=media DARK_MODE=media expo run:android",
    "ios": "DARK_MODE=media DARK_MODE=media expo run:ios",
    "web": "DARK_MODE=media DARK_MODE=media expo start --web",
    "test": "bun jest --watchAll",
    "lint:eslint": "bun eslint . --cache --max-warnings 0 ",
    "lint:formatting": "bun prettier --check \"**/*.{js,jsx,ts,tsx,json,md,css,scss}\"",
    "lint": "bun run lint:eslint && bun run lint:formatting",
    "format": "bun prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css,scss}\" && bun eslint . --fix --cache",
    "check": "tsc --noEmit --skipLibCheck",
    "validate": "bun run format && bun run lint",
    "validate:strict": "bun run validate && bun run check",
    "index-images": "bun ./util/indexImages.ts",
    "dump-modules": "rm -rf node_modules android ios dist web-build",
    "reboot-emulator": "adb -e reboot",
    "reset-simulator": "xcrun simctl erase all && sudo xcode-select --reset && watchman watch-del-all",
    "expo-doctor": "bun x expo-doctor",
    "expo-fingerprint": "bun x @expo/fingerprint . > fingerprint.json",
    "regenerate-nativewind-styles": "bun x tailwindcss -i ./global.css -o ./node_modules/.cache/nativewind/global.css",
    "fix-versions": "bun x expo@latest install --fix",
    "db:generate-types": "supabase gen types typescript --local > database/database.types.ts",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:init": "supabase init",
    "db:migrate:local": "supabase migration up --local",
    "db:reset": "supabase db reset"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg))"
    ]
  },
  "dependencies": {
    "@expo/html-elements": "0.4.2",
    "@expo/vector-icons": "^14.1.0",
    "@gluestack-ui/accordion": "^1.0.14",
    "@gluestack-ui/actionsheet": "^0.2.53",
    "@gluestack-ui/alert": "^0.1.16",
    "@gluestack-ui/alert-dialog": "^0.1.38",
    "@gluestack-ui/avatar": "^0.1.18",
    "@gluestack-ui/button": "^1.0.14",
    "@gluestack-ui/checkbox": "^0.1.39",
    "@gluestack-ui/divider": "^0.1.10",
    "@gluestack-ui/fab": "^0.1.28",
    "@gluestack-ui/form-control": "^0.1.19",
    "@gluestack-ui/icon": "^0.1.27",
    "@gluestack-ui/image": "^0.1.17",
    "@gluestack-ui/input": "^0.1.38",
    "@gluestack-ui/link": "^0.1.29",
    "@gluestack-ui/menu": "^0.2.43",
    "@gluestack-ui/modal": "^0.1.41",
    "@gluestack-ui/nativewind-utils": "^1.0.26",
    "@gluestack-ui/overlay": "^0.1.22",
    "@gluestack-ui/popover": "^0.1.49",
    "@gluestack-ui/pressable": "^0.1.23",
    "@gluestack-ui/progress": "^0.1.18",
    "@gluestack-ui/radio": "^0.1.40",
    "@gluestack-ui/select": "^0.1.31",
    "@gluestack-ui/slider": "^0.1.32",
    "@gluestack-ui/spinner": "^0.1.15",
    "@gluestack-ui/switch": "^0.1.29",
    "@gluestack-ui/textarea": "^0.1.25",
    "@gluestack-ui/toast": "^1.0.9",
    "@gluestack-ui/tooltip": "^0.1.44",
    "@legendapp/motion": "^2.4.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.13",
    "@react-navigation/native": "^7.1.9",
    "@rneui/themed": "^4.0.0-rc.8",
    "@supabase/realtime-js": "^2.11.8",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "2.49.6",
    "babel-plugin-module-resolver": "^5.0.2",
    "expo": "~53.0.9",
    "expo-blur": "~14.1.4",
    "expo-constants": "~17.1.6",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.0.7",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.7",
    "expo-web-browser": "~14.1.6",
    "jotai": "^2.12.4",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-css-interop": "^0.1.22",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "^3.17.5",
    "react-native-safe-area-context": "^5.4.1",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "15.2.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "tailwindcss": "3.4.17"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@expo/fingerprint": "~0.12.4",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.14",
    "@types/react-test-renderer": "^18.3.1",
    "eslint": "^9.27.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "glob": "^10.4.5",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.5",
    "jscodeshift": "0.15.2",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-test-renderer": "18.3.1",
    "supabase": "^2.23.4",
    "typescript": "~5.8.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "private": true,
  "patchedDependencies": {
    "@gluestack-ui/nativewind-utils@1.0.26": "patches/@gluestack-ui%2Fnativewind-utils@1.0.26.patch"
  }
}
