{
  "name": "app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "prebuild": "expo prebuild",
    "android": "DARK_MODE=media expo run:android",
    "ios": "DARK_MODE=media expo run:ios",
    "web": "DARK_MODE=media expo start --web",
    "test": "bun jest --watchAll",
    "lint:eslint": "bun eslint . --cache --max-warnings 0 ",
    "lint:formatting": "bun prettier --check \"**/*.{js,jsx,ts,tsx,json,md,css,scss}\"",
    "lint": "bun run lint:eslint && bun run lint:formatting",
    "format": "bun prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css,scss}\" && bun eslint . --fix --cache",
    "check": "tsc --noEmit --skipLibCheck",
    "validate": "bun run format && bun run lint",
    "validate:strict": "bun run validate && bun run check",
    "index-images": "bun ./util/indexImages.ts",
    "dump-modules": "rm -rf node_modules android ios dist web-build",
    "reboot-emulator": "adb -e reboot",
    "reset-simulator": "xcrun simctl erase all && sudo xcode-select --reset && watchman watch-del-all",
    "expo-doctor": "bun x expo-doctor",
    "expo-fingerprint": "bun x @expo/fingerprint . > fingerprint.json",
    "regenerate-nativewind-styles": "bun x tailwindcss -i ./global.css -o ./node_modules/.cache/nativewind/global.css",
    "fix-versions": "bun x expo@latest install --fix",
    "db:generate-types": "supabase gen types typescript --local > database/database.types.ts",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:init": "supabase init"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg))"
    ]
  },
  "dependencies": {
    "@expo/html-elements": "0.4.2",
    "@expo/vector-icons": "^14.0.2",
    "@gluestack-ui/accordion": "^1.0.8",
    "@gluestack-ui/actionsheet": "^0.2.46",
    "@gluestack-ui/alert": "^0.1.16",
    "@gluestack-ui/alert-dialog": "^0.1.32",
    "@gluestack-ui/avatar": "^0.1.18",
    "@gluestack-ui/button": "^1.0.8",
    "@gluestack-ui/checkbox": "^0.1.33",
    "@gluestack-ui/divider": "^0.1.10",
    "@gluestack-ui/fab": "^0.1.22",
    "@gluestack-ui/form-control": "^0.1.19",
    "@gluestack-ui/icon": "^0.1.25",
    "@gluestack-ui/image": "^0.1.11",
    "@gluestack-ui/input": "^0.1.32",
    "@gluestack-ui/link": "^0.1.23",
    "@gluestack-ui/menu": "^0.2.37",
    "@gluestack-ui/modal": "^0.1.35",
    "@gluestack-ui/nativewind-utils": "^1.0.26",
    "@gluestack-ui/overlay": "^0.1.16",
    "@gluestack-ui/popover": "^0.1.43",
    "@gluestack-ui/pressable": "^0.1.17",
    "@gluestack-ui/progress": "^0.1.18",
    "@gluestack-ui/radio": "^0.1.34",
    "@gluestack-ui/select": "^0.1.30",
    "@gluestack-ui/slider": "^0.1.26",
    "@gluestack-ui/spinner": "^0.1.15",
    "@gluestack-ui/switch": "^0.1.23",
    "@gluestack-ui/textarea": "^0.1.24",
    "@gluestack-ui/toast": "^1.0.8",
    "@gluestack-ui/tooltip": "^0.1.38",
    "@legendapp/motion": "^2.4.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@rneui/themed": "^4.0.0-rc.8",
    "@supabase/supabase-js": "^2.48.1",
    "@supabase/ssr": "^0.5.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "expo": "~52.0.35",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.6",
    "expo-font": "~13.0.3",
    "expo-haptics": "~14.0.1",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.17",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.8",
    "expo-web-browser": "~14.0.2",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-css-interop": "^0.1.22",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "^3.16.7",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "tailwindcss": "3.4.17"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/fingerprint": "^0.11.10",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "eslint-plugin-react": "^7.37.4",
    "glob": "^10.3.10",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.4",
    "jscodeshift": "0.15.2",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3",
    "supabase": "^2.12.1"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "private": true
}
